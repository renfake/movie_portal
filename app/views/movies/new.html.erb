<div id="content-header">
  <h1>新建电影</h1>
</div>
<div class="container-fluid" >
  <div class="row-fluid">
    <div class="span12">
      <%= form_for :movie, :html => {:id => 'new_movie', :class => "form-horizontal"} do |f| %>
          <%= render :partial => "form", :object => f %>

          <div class="form-actions">
            <%= f.button "Create", :class => 'btn btn-primary' %>
            <%= f.button 'Cancel', :class => 'btn', :type => 'reset' %>
          </div>
      <% end %>

    </div>
  </div>
</div>